}

.serviceItemschild_msg_pric {
  height: 100rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(236, 132, 1, 1);
  line-height: 41rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.serviceItemschild_msg_btn {
  width: 136rpx;
  height: 54rpx;
  border: 1rpx solid #ECAC01;
  background: rgba(255, 255, 255, 1);
  border-radius: 6rpx;
  font-size: 26rpx;
  font-family: PingFang SC;
  font-weight: bold;
  display: flex;
  align-items: center;
  justify-content: center;
}

