.ServiceAddress {
  width: 100%;
  height: 100%;
  padding: 23rpx 12rpx 0 12rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: start;

}

.ServiceAddress_card {

  box-sizing: border-box;
  padding: 29rpx 17rpx 0rpx 20rpx;
  margin-bottom: 25rpx;
  width: 727rpx;
  height: 266rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 19px 0px rgba(0, 0, 0, 0.09);
  border-radius: 9px;
}

.ServiceAddress_card_top {
  width: 100%;
  box-sizing: border-box;
  padding: 0 13rpx 18rpx 21rpx;
  border-bottom: 1px solid rgba(215, 215, 215, 1);
}

.ServiceAddress_card_name {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 10rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(60, 60, 60, 1);

}

.ServiceAddress_card_add {
  font-size: 24rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  letter-spacing: 3rpx;
  line-height: 40rpx;
}

.ServiceAddress_card_bottom {
  height: 87rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #FFA036;
  line-height: 23px;


}

.ServiceAddress_card_bottom_Left {
  margin-left: 21rpx;
}

.ServiceAddress_card_bottom_Right {
  color: #3C3C3C;
  font-family: PingFang SC;
  font-weight: 500;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.ServiceAddress_card_bottom_Rightbox {
  margin-right: 20rpx;
}

.AddAddress {
  position: fixed;
  width: 100%;
  height: 90rpx;
  bottom: 96rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.AddAddress_child {
  width: 690rpx;
  height: 90rpx;
  background: #FC923C;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  
}