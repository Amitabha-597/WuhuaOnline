.MyCollage_container {
  height: 100%;
  min-height: 100%;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.MyCollage {
  background: #F0F2F5;
}

.title {
  width: 100%;
  height: 93rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-around;
}

.titleSel {
  height: 93rpx;
  width: 20%;
  color: #A9A9A9;
  font-size: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-family: PingFang SC;
  font-weight: bold;
}

.titleSel_name {
  height: 88rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  font-weight: 400;
  justify-content: center;
}

.headerLineSel {
  background: #EC8401;
  height: 4rpx;
  width: 53rpx;
  position: relative;
}

.headerLineUnsel {
  background: #fff;
  height: 4rpx;
  width: 53rpx;
  position: relative;
}

.swiper {
  width: 100%;
  flex: 1;
  height: 93.6vh;
  overflow: scroll;
}

.MyCollage_all {
  margin-top: 20rpx;
  width: 750rpx;
  height: 322rpx;
  background: rgba(255, 255, 255, 1);
  padding: 20rpx 30rpx 0 30rpx;
  box-sizing: border-box;
}

.MyCollage_all_item {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  border-bottom: 1rpx solid #f1f2f2;
}

.MyCollage_all_item_imgbox {
  width: 201rpx;
  height: 186rpx;
  background: greenyellow;
  position: relative;
}

.MyCollage_outtime {
  width: 200rpx;
  height: 40rpx;
  position: absolute;
  bottom: 0;
  background: red;
  background-image: linear-gradient(to right, #EC8402, #F1CD9F);
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-left: 10rpx;
  box-sizing: border-box;
}


.MyCollage_all_item_text {
  height: 186rpx;
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-left: 30rpx;
  justify-content: space-around;
}

.MyCollage_all_item_text {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
  line-height: 41rpx;

}



.MyCollage_all_item_text_msg>view>text {
  margin-right: 20rpx;
}

.MyCollage_all_item_text_msg {
  display: flex;
  justify-content: space-between;
}

.MyCollage_all_btnbox {
  width: 100%;
  height: 99rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.MyCollage_all_btnbox>view {
  width: 180rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 20rpx;

}

.MyCollage_all_btnA {
  border: 1rpx solid #A9A9A9;
  color: #646464;
}

.MyCollage_all_btnB {
  color: #fff;
  background: #EC8401;
  margin-left: 20rpx;
}

.AddCollage {
  flex: 1;
}

.MyCollage_box {
  width: 100%;
  height: 100%;
  flex-direction: column;
  justify-content: flex-start;
  display: flex;
}

.AddCollage {
  display: flex;
  align-items: center;
  justify-content: center;
}

.AddCollage_item {
  width: 266rpx;
  height: 60rpx;
  border: 1rpx solid #A9A9A9;
  border-radius: 30rpx;
  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(166, 166, 166, 1);
  display: flex;
  align-items: center;
  justify-content: center;
}