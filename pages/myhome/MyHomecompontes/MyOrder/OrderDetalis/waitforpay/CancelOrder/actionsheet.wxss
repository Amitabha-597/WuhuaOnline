.weui-mask {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6)
}

.weui-mask_transparent {
  position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0
}

.weui-actionsheet__cell_jiesao {
  margin: 0 auto;
  width: 688rpx;
  padding: 31rpx 15rpx 32rpx 25rpx;
  box-sizing: border-box;
  height: 131rpx;
  background: #E7E7E8;
  font-size: 26rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 41rpx;
}

.weui-actionsheet__cell_xzyy {

  font-size: 26rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 41rpx;
  margin: 33rpx auto;
  text-align: center;
}

.weui-actionsheet {
  position: fixed;
  left: 0;
  bottom: 0;
  transform: translate(0, 100%);
  backface-visibility: hidden;
  z-index: 5000;
  width: 100%;
  background-color: #EAE7E8;
  transition: transform .3s;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  overflow: hidden
}

.weui-actionsheet__title {
  position: relative;
  height: 36px;
  padding: 0 24px;
  display: flex;
  justify-content: center;
  flex-direction: column;
  text-align: center;
  font-size: 12px;
  color: rgba(0, 0, 0, 0.5);
  line-height: 1.4;
  background: #FFFFFF
}

.weui-actionsheet__title:before {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1)
}

.weui-actionsheet__title {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: flex;
  align-items: center;
  justify-content: center;
}

.actionsheet__title_box {
  width: 70rpx;
  height: 10rpx;
  background: #f3f3f3;
}

.weui-actionsheet__menu {
  color: rgba(0, 0, 0, 0.9);
  background-color: #FFFFFF
}

.weui-actionsheet__action {
  padding-top: 30rpx;
  background-color: #FFFFFF;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom)
}

.weui-actionsheet__cell {
  position: relative;
  padding: 10px 60rpx;
  text-align: center;
  line-height: 1.41176471;
  font-size: 26rpx;
  font-family: SimHei;
  font-weight: 400;

}

.reason {
  margin: 0 auto;
  width: 680rpx;
  height: 150rpx;
  border: 1rpx solid #f1f1f2;
}

.reason_conent {
  height: 110rpx;
  font-size: 26rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(170, 170, 170, 1);
  line-height: 30rpx;
  width: 100%;
  padding:20rpx 20rpx 0 20rpx;
  box-sizing: border-box;
}

.reason_num {
  height: 40rpx;
  font-size: 26rpx;
  font-family: SimHei;
  font-weight: 400;
  color: rgba(236, 133, 2, 1);
  text-align: right;
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 30rpx;
}

.weui-actionsheet__coupon {
  display: flex;
  justify-content: space-between;

}

.weui-actionsheet__couponicon {
  width: 24rpx;
  height: 24rpx;
  /* border: 3rpx solid #CECECE; */
  border-radius: 50%;

}

.actionsheet__cell_tiele {

  font-size: 30rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.weui-actionsheet__cell:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1rpx solid rgba(0, 0, 0, 0.1);
  color: rgba(0, 0, 0, 0.1)
}

.weui-actionsheet__cell:active {
  background-color: #ECECEC
}

.weui-actionsheet__cell:first-child:before {
  display: none
}

.weui-actionsheet__cell_warn {
  color: #EC8401
}

.weui-skin_android .weui-actionsheet {
  position: fixed;
  left: 50%;
  top: 50%;
  bottom: auto;
  transform: translate(-50%, -50%);
  width: 274px;
  box-sizing: border-box;
  backface-visibility: hidden;
  background: transparent;
  transition: transform .3s;
  border-radius: 2px
}

.weui-skin_android .weui-actionsheet__action {
  display: none
}

.weui-skin_android .weui-actionsheet__menu {
  border-radius: 2px;
  box-shadow: 0 6px 30px 0 rgba(0, 0, 0, 0.1)
}

.weui-skin_android .weui-actionsheet__cell {
  padding: 16px;
  font-size: 17px;
  line-height: 1.41176471;
  color: rgba(0, 0, 0, 0.9);
  text-align: left
}

.weui-skin_android .weui-actionsheet__cell:first-child {
  border-top-left-radius: 2px;
  border-top-right-radius: 2px
}

.weui-skin_android .weui-actionsheet__cell:last-child {
  border-bottom-left-radius: 2px;
  border-bottom-right-radius: 2px
}

.weui-actionsheet_toggle {
  transform: translate(0, 0)
}

.weui-mask.weui-mask_hidden {
  opacity: 0;
  transform: scale3d(1, 1, 0)
}

.weui-mask {
  opacity: 1;
  transform: scale3d(1, 1, 1);
  transition: all .3s
}



.weui-actionsheet_finishchild {
  width: 623rpx;
  height: 80rpx;
  background: #F0931D;
  color: #fff;
  border-radius: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  font-family: SimHei;
  font-weight: 400;
  letter-spacing: 5rpx;
}