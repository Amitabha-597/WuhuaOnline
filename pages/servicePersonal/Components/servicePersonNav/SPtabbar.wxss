.navbox {
  width: 100%;
  height: 144rpx;
  display: flex;
  align-items: flex-end;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
}

.navItem {
  height: 108rpx;
  width: 100%;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-around;
  box-shadow: 0px -5px 5px #F5F5F5;
}

.navItem>view {
  width: 33.33%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  
}

.navItem>view>text {
  padding: 5rpx 0;
  font-size: 20rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: rgba(241, 132, 1, 1);
}

.order {
  width: 150rpx;
  height: 130rpx;
  margin-top: -36rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #fff;
  border-radius: 50%;
  /* box-shadow: 0px -10px 5px #F5F5F5; */
  box-shadow: 0px -5px 5px #F5F5F5;
}

