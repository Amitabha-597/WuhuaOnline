.Account{
  width: 100%;
  height: 100vh;
  padding:21rpx 30rpx ;
  background: #f1f2f2;
  box-sizing: border-box;
  font-size:30rpx;
  font-family:SimHei;
  font-weight:400;
  color:rgba(51,51,51,1)
}
.Accountmsg{
padding: 10rpx 0;
box-sizing: border-box;
  width: 100%;
  height: 330rpx;
  background: #fff;
  border-radius:8px;
}
.Accountmsg>view{
  height: 33.33%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 15rpx 0 11rpx;
  box-sizing: border-box;
  background: #fff;
}
.avatarbox{
  width: 103rpx;
  height: 103rpx;
  background: red;
  border-radius: 50%;
}
.company{
  margin-top: 21rpx;
 width: 100%;
 height: 310rpx; 
 background: #fff;

}
.company>view{
  height: 33.33%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 12rpx;
  box-sizing: border-box;
}
.outlogin{
  margin-top: 21rpx;
  width: 100%;
  height: 166rpx;
  background:#fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
.outlogin_btn{
  width: 443rpx;
  height: 86rpx;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size:30rpx;
font-family:PingFang SC;
font-weight:bold;
color:rgba(254,254,254,1);
line-height:41px;
  background-image: linear-gradient(#F6B701, #F18401);
}